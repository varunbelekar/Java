public class LambdaDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		A obj;
		
		//Anonymous Class
		/*obj=new A(){
			public void show(){
				System.out.println("Hello");
			}
		};*/
		
		obj=() -> System.out.println("Hello1");  //Lambda Expression		
		obj.show();
	}

}
